{
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },

    "AllowedHosts": "*",

    "mysql": {
        "client": {
            "sslmode": "none"
        }
    },

    "spring": {
        "application": {
            "name": "river-flow-api"
        }
    },

    "eureka": {
        "client": {
            "serviceUrl": "http://localhost:8761/eureka/",
            "shouldFetchRegistry": false,
            "ValidateCertificates": false
        },
        "instance": {
            "port": 5000
        }
    }
}